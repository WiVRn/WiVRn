#: client/scenes/lobby_gui.cpp:948
msgid "Use edge direction"
msgstr "Použitie smeru okrajov"

#: client/scenes/lobby_gui.cpp:954
msgid "Adds an additional performance cost"
msgstr "Pridáva dodatočné náklady na výkon"

#: client/scenes/lobby_gui.cpp:959
